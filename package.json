{
  "name": "dgagent",
  "private": true,
  "version": "1.0.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 tsc -p electron/tsconfig.json && electron . --dev",
    "build": "tsc && vite build && tsc -p electron/tsconfig.json && electron-builder",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.deepgenome.agent",
    "productName": "DGAgent",
    "files": [
      "dist-electron/**/*",
      "dist/**/*",
      "package.json"
    ],
    "asarUnpack": [
      "node_modules/@lancedb/**/*",
      "node_modules/@ffmpeg-installer/**/*"
    ],
    "buildDependenciesFromSource": false,
    "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
    "directories": {
      "output": "release"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "DGAgent"
    }
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@lancedb/lancedb": "^0.22.3",
    "@langchain/community": "^1.0.4",
    "@langchain/core": "^1.0.6",
    "@langchain/openai": "^1.1.2",
    "@langchain/textsplitters": "^1.0.0",
    "apache-arrow": "^21.1.0",
    "chokidar": "^4.0.3",
    "clsx": "^2.1.0",
    "dotenv": "^17.2.3",
    "duck-duck-scrape": "^2.2.7",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.5",
    "highlight.js": "^11.11.1",
    "langchain": "^1.0.6",
    "lucide-react": "^0.344.0",
    "mammoth": "^1.11.0",
    "mic": "^2.1.2",
    "minisearch": "^7.2.0",
    "node-tesseract-ocr": "^2.2.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^5.4.394",
    "pptx2json": "^0.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "tailwind-merge": "^2.2.1",
    "tesseract.js": "^6.0.1",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^20.11.20",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "wait-on": "^7.2.0"
  }
}
